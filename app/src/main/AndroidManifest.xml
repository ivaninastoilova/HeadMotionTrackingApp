<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    package="com.example.userinterface.userinterface">
    <uses-sdk tools:overrideLibrary="com.st.BlueSTSDK" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme"
        >
        <activity android:name=".MainActivity"
            android:label="Welcome">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <activity
            android:name=".TrainingRound"
            android:label="Training Round"
            >
            <intent-filter>
                <action android:name="android.intent.action.TrainingRound"/>
                    <category android:name="android.intent.category.DEFAULT"/>
            </intent-filter>
        </activity>
        <activity android:name=".GamesLevels"
            android:label="Games Levels">
            <intent-filter>
                <action android:name="android.intent.action.GamesLevels"/>
                <category android:name="android.intent.category.DEFAULT"/>
            </intent-filter>
        </activity>

        <activity android:name=".LevelOne"
            android:label="Level 1">
            <intent-filter>
                <action android:name="android.intent.action.LevelOne"/>
                <category android:name="android.intent.category.DEFAULT"/>
            </intent-filter>
        </activity>

        <activity android:name=".LevelTwo"
            android:label="Level Two">
            <intent-filter>
                <action android:name="android.intent.action.LevelTwo"/>
                <category android:name="android.intent.category.DEFAULT"/>
            </intent-filter>
        </activity>

        <activity android:name=".LevelThree"
            android:label="Level Three">
            <intent-filter>
                <action android:name="android.intent.action.LevelThree"/>
                <category android:name="android.intent.category.DEFAULT"/>
            </intent-filter>
        </activity>

    </application>

</manifest>